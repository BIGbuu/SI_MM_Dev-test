--
-- Структура таблицы `twitterlenta`
--

CREATE TABLE IF NOT EXISTS `twitterlenta` (
  `id` int(11) NOT NULL,
  `tweets` int(11) default NULL,
  `followings` int(11) default NULL,
  `followers` int(11) default NULL,
  `lastparse` timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
  `parse_time` int(20) default NULL,
  UNIQUE KEY `id` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=cp1251;

